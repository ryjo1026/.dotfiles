[user]
	name = Ryan Johnston
	email = mrynjoh@amazon.com
[core]
	editor = vim
	pager = less -FMRiX
	program = gpg
	excludesfile = ~/.gitignore
[commit]
    template = ~/.gitmessage
[tag]
	forceSignAnnotated = false
[push]
	default = simple
[pull]
	ff = only
[color]
	ui = auto
[alias]
	dag = log --graph --format='format:%C(yellow)%h%C(reset) %C(blue)%an <%ae>%C(reset) %C(magenta)%cr%C(reset)%C(auto)%d%C(reset)%n%s' --date-order
